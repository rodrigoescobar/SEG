package com.seg;

/**
 * @author SEGg9
 * 
 * This class is used to store all the necessary information about a taxiway-runway join, which connects a 
 * {@link TaxiwaySection} to a {@link Runway}
 */
public class TaxiwayRunwayJoin extends TaxiwayJoin{
    public final int joinRunway;

    TaxiwayRunwayJoin(int uid, double x, double y, int rw) {
        super(uid, x, y);
        joinRunway = rw;
    }
}