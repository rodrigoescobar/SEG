package com.seg;

/**
 * @author SEGg9
 * 
 * This class is used to store all the necessary information about a taxiway section.
 */
public class TaxiwaySection
{
    final int UID;
    final int sectionStartID;
    final int sectionEndID;
    public final int sectionWidth;

    TaxiwaySection (int uid, int s, int e, int w)
    {
        UID = uid;
        sectionStartID = s;
        sectionEndID = e;
        sectionWidth = w;
    }
}